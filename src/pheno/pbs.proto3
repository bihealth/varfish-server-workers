syntax = "proto3";

package varfish_server_worker.pheno.pbs;

// Stores the empirical cumulative distribution function for a gene.
message SimulationResults {
    // The NCBI gene id.
    uint32 ncbi_gene_id = 1;
    // The gene symbol.
    string gene_symbol = 2;
    // The number of terms.
    uint32 term_count = 3;
    // The sorted scores.
    repeated float scores = 4;
}
